{
    "Simple Project Start":{
        "prefix": "simpleProjectStart",
        "body":[
            "STRUDL '${1:Problem Name}' '${2:Title}'",
            "",
            "$-----------------------------------------------------------------------------------",
            "$ Define input units",
            "$-----------------------------------------------------------------------------------",
            "UNITS ${3:Length Unit} ${4:Force Unit} ${5:Angular Unit}",
            "",
            "JOINT COORDINATES",
            "1      X 0     Y 0     Z 0     S",
            "2      X 5     Y 0     Z 0     S",
            "",
            "TYPE ${6:Plane/Space} ${7:Truss/Frame/Grid}",
            "",
            "JOINT RELEASES",
            "",
            "MEMBER INCIDENCES",
            "1      1       2",
            "",
            "MATERIAL STEEL ALL MEMBERS",
            "",
            "MEMBER PROPERTIES PRISMATIC",
            "   1   AX 100.0 IX 100.0 IY 100.0 IZ 100.0",
            "",
            "LOADING 1 'Load Name'",
            "JOINT LOADS",
            "   1 FORCE Y -20.0",
            "",
            "STIFFNESS ANALYSIS"
        ]
    }
}